					function = "gpio";
				};

				config {
					pins = "gpio120";
					drive-strength = <8>; /* 8 mA */
					bias-pull-down; /* PULL DOWN */
					output-low;
				};
			};

			cross_conn_det_sus: lines_off {
				mux {
					pins = "gpio120";
					function = "gpio";
				};

				config {
					pins = "gpio120";
					drive-strength = <2>; /* 2 mA */
