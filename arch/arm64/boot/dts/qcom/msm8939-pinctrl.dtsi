&soc {
	msm_gpio: pinctrl@1000000 {
		compatible = "qcom,msm8939-pinctrl";
		reg = <0x1000000 0x300000>;
		interrupts = <0 208 0>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;

		/* Kanuti HW ID */
		kanuti_hwid: kanuti_hwid {
			mux {
				pins = "gpio0", "gpio1", "gpio2", "gpio3",
					"gpio8", "gpio9", "gpio107", "gpio35";
				function = "gpio";
			};

			config {
				pins = "gpio0", "gpio1", "gpio2", "gpio3",
					"gpio8", "gpio9", "gpio107", "gpio35";
				drive-strength = <2>; /* 2 mA */
				bias-disable; /* No PULL */
			};
		};

		pmx-uartconsole {
			uart_console_sleep: uart_console_sleep {
				mux {
					pins = "gpio4", "gpio5";
					function = "blsp_uart2";
				};

				config {
					pins = "gpio4", "gpio5";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* PULL DOWN */
				};
			};
		};

		pmx_rd_nfc_int {
			nfc_int_active: active {
				mux {
					pins = "gpio21";
					function = "gpio";
				};

				config {
					pins = "gpio21";
					drive-strength = <6>; /* 6 mA */
					bias-pull-up; /* PULL UP */
				};
			};

			nfc_int_suspend: suspend {
				mux {
					pins = "gpio21";
					function = "gpio";
				};

				config {
					pins = "gpio21";
					drive-strength = <6>; /* 6 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		pmx_nfc_reset {
			nfc_disable_active: active {
				mux {
					pins = "gpio20";
					function = "gpio";
				};

				config {
					pins = "gpio20";
					driver-strength = <6>; /* 6 mA */
					bias-pull-up; /* PULL UP */
				};
			};

			nfc_disable_suspend: suspend {
				mux {
					pins = "gpio20";
					function = "gpio";
				};

				config {
					pins = "gpio20";
					driver-strength = <6>; /* 6 mA */
					bias-disable; /* No PULL */
				};
			};
		};

		/* HS UART CONFIGURATION */
		blsp1_uart1_active: blsp1_uart1_active {
			mux {
				pins = "gpio0", "gpio1", "gpio2", "gpio3";
				function = "blsp_uart1";
			};

			config {
				pins = "gpio0", "gpio1", "gpio2", "gpio3";
				drive-strength = <16>; /* 16 mA */
				bias-disable; /* No PULL */
			};
		};

		blsp1_uart1_sleep: blsp1_uart1_sleep {
			mux {
				pins = "gpio0", "gpio1", "gpio2", "gpio3";
				function = "gpio";
			};

			config {
				pins = "gpio0", "gpio1", "gpio2", "gpio3";
				drive-strength = <2>; /* 2 mA */
				bias-disable; /* No PULL */
			};
		};

		sdhc2_cd_pin {
			sdc2_cd_on: cd_on {
				mux {
					pins = "gpio38";
					function = "gpio";
				};

				config {
					pins = "gpio38";
					drive-strength = <2>; /* 2 mA */
					bias-pull-up; /* PULL UP */
				};
			};

			sdc2_cd_off: cd_off {
				mux {
					pins = "gpio38";
					function = "gpio";
				};

				config {
					pins = "gpio38";
					drive-strength = <2>; /* 2 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		/* SDC pinmux */
		pmx_sdc1_clk {
			sdc1_clk_on: sdc1_clk_on {
				config {
					pins = "sdc1_clk";
					drive-strength = <16>; /* 16 mA */
					bias-disable; /* No PULL */
				};
			};

			sdc1_clk_off: sdc1_clk_off {
				config {
					pins = "sdc1_clk";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};
		};

		pmx_sdc1_cmd {
			sdc1_cmd_on: sdc1_cmd_on {
				config {
					pins = "sdc1_cmd";
					drive-strength = <10>; /* 10 mA */
					bias-pull-up; /* PULL UP */
				};
			};

			sdc1_cmd_off: sdc1_cmd_off {
				config {
					pins = "sdc1_cmd";
					drive-strength = <2>; /* 2 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		pmx_sdc1_data {
			sdc1_data_on: sdc1_data_on {
				config {
					pins = "sdc1_data";
					drive-strength = <10>; /* 10 mA */
					bias-pull-up; /* PULL UP */
				};
			};

			sdc1_data_off: sdc1_data_off {
				config {
					pins = "sdc1_data";
					drive-strength = <2>; /* 2 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		pmx_sdc2_clk {
			sdc2_clk_on: sdc2_clk_on {
				config {
					pins = "sdc2_clk";
					drive-strength = <10>; /* 10 mA */
					bias-disable; /* No PULL */
				};
			};

			sdc2_clk_off: sdc2_clk_off {
				config {
					pins = "sdc2_clk";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};
		};

		pmx_sdc2_cmd {
			sdc2_cmd_on: sdc2_cmd_on {
				config {
					pins = "sdc2_cmd";
					drive-strength = <10>; /* 10 mA */
					bias-pull-up; /* PULL UP */
				};
			};

			sdc2_cmd_off: sdc2_cmd_off {
				config {
					pins = "sdc2_cmd";
					drive-strength = <2>; /* 2 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		pmx_sdc2_data {
			sdc2_data_on: sdc2_data_on {
				config {
					pins = "sdc2_data";
					drive-strength = <10>; /* 10 mA */
					bias-pull-up; /* PULL UP */
				};
			};

			sdc2_data_off: sdc2_data_off {
				config {
					pins = "sdc2_data";
					drive-strength = <2>; /* 2 mA */
					bias-pull-up; /* PULL UP */
				};
			 };
		};

		/* I2C CONFIGURATION */
		i2c_0 {
			i2c_0_active: i2c_0_active {
				mux {
					/* SDA, SCL */
					pins = "gpio6", "gpio7";
					function = "blsp_i2c2";
				};

				config {
					pins = "gpio6", "gpio7";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};

			i2c_0_sleep: i2c_0_sleep {
				mux {
					/* SDA, SCL */
					pins = "gpio6", "gpio7";
					function = "blsp_i2c2";
				};

				config {
					pins = "gpio6", "gpio7";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};
		};

		i2c_4 {
			i2c_4_active: i2c_4_active {
				mux {
					/* SDA, SCL */
					pins = "gpio14", "gpio15";
					function = "blsp_i2c4";
				};

				config {
					pins = "gpio14", "gpio15";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};

			i2c_4_sleep: i2c_4_sleep {
				mux {
					/* SDA, SCL */
					pins = "gpio14", "gpio15";
					function = "blsp_i2c4";
				};

				config {
					pins = "gpio14", "gpio15";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};
		};

		i2c_5 {
			i2c_5_active: i2c_5_active {
				mux {
					/* SDA, SCL */
					pins = "gpio18", "gpio19";
					function = "blsp_i2c5";
				};

				config {
					pins = "gpio18", "gpio19";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};

			i2c_5_sleep: i2c_5_sleep {
				mux {
					/* SDA, SCL */
					pins = "gpio18", "gpio19";
					function = "blsp_i2c5";
				};

				config {
					pins = "gpio18", "gpio19";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};
		};

		i2c_6 {
			i2c_6_active: i2c_6_active {
				mux {
					/* SDA, SCL */
					pins = "gpio22", "gpio23";
					function = "blsp_i2c6";
				};

				config {
					pins = "gpio22", "gpio23";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};

			i2c_6_sleep: i2c_6_sleep {
				mux {
					/* SDA, SCL */
					pins = "gpio22", "gpio23";
					function = "blsp_i2c6";
				};

				config {
					pins = "gpio22", "gpio23";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
				};
			};
		};

		/* SMB1360 */
		smb_int_pin {
			smb_int_default: smb_int_default {
				mux {
					pins = "gpio62";
					function = "gpio";
				};

				config {
					pins = "gpio62";
					drive-strength = <2>; /* 2 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		/* USB ID */
		usb-id-pin {
			usbid_default: default {
				mux {
					pins = "gpio110";
					function = "gpio";
				};

				config {
					pins = "gpio110";
					drive-strength = <8>; /* 8 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		uicc {
			uicc_active: active {
				mux {
					pins = "gpio53", "gpio54";
					function = "uim2";
				};
				config {
					pins = "gpio53", "gpio54";
					drive-strength = <8>; /* 8 mA */
					bias-disable; /* No PULL */
				};
			};

			uicc_sleep: sleep {
				mux {
					pins = "gpio53", "gpio54";
					function = "uim2";
				};
				config {
					pins = "gpio53", "gpio54";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* PULL DOWN */
				};
			};
		};

		wcnss_pmux_5wire {
			wcnss_default: wcnss_default {
				mux {
					pins = "gpio40", "gpio41",
							"gpio42", "gpio43",
							"gpio44";
					function = "wcss_wlan";
				};

				config {
					pins = "gpio40", "gpio41",
							"gpio42", "gpio43",
							"gpio44";
					drive-strength = <6>; /* 6 mA */
					bias-pull-up; /* PULL UP */
				};
			};

			wcnss_sleep: wcnss_sleep {
				mux {
					pins = "gpio40", "gpio41",
							"gpio42", "gpio43",
							"gpio44";
					function = "wcss_wlan";
				};

				config {
					pins = "gpio40", "gpio41",
							"gpio42", "gpio43",
							"gpio44";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* PULL DOWN */
				};
			};
		};

		wcnss_pmux_gpio: wcnss_pmux_gpio {
			wcnss_gpio_default: wcnss_gpio_default {
				mux {
					pins = "gpio40", "gpio41",
							"gpio42", "gpio43",
							"gpio44";
					function = "gpio";

				};

				config {
					pins = "gpio40", "gpio41",
							"gpio42", "gpio43",
							"gpio44";
					drive-strength = <6>; /* 6 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		tlmm_gpio_key {
			gpio_key_active: gpio_key_active {
				mux {
					pins = "gpio108", "gpio109", "gpio51", "gpio11";
					function = "gpio";
				};

				config {
					pins = "gpio108", "gpio109", "gpio51", "gpio11";
					drive-strength = <2>; /* 2 mA */
					bias-pull-up; /* PULL UP */
				};
			};

			gpio_key_suspend: gpio_key_suspend {
				mux {
					pins = "gpio108", "gpio109", "gpio51", "gpio11";
					function = "gpio";
				};

				config {
					pins = "gpio108", "gpio109", "gpio51", "gpio11";
					drive-strength = <2>; /* 2 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		/* add pingrp for touchscreen */
		pmx_ts_int_active {
			ts_int_active: ts_int_active {
				mux {
					pins = "gpio13";
					function = "gpio";
				};

				config {
					pins = "gpio13";
					drive-strength = <16>; /* 16 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		pmx_ts_int_suspend {
			ts_int_suspend: ts_int_suspend {
				mux {
					pins = "gpio13";
					function = "gpio";
				};

				config {
					pins = "gpio13";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* PULL DOWN */
				};
			};
		};

		pmx_ts_reset_active {
			ts_reset_active: ts_reset_active {
				mux {
					pins = "gpio12";
					function = "gpio";
				};

				config {
					pins = "gpio12";
					drive-strength = <16>; /* 16 mA */
					bias-pull-up; /* PULL UP */
				};
			};
		};

		pmx_ts_reset_suspend {
			ts_reset_suspend: ts_reset_suspend {
				mux {
					pins = "gpio12";
					function = "gpio";
				};

				config {
					pins = "gpio12";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* PULL DOWN */
				};
			};
		};

		pmx_ts_release {
			ts_release: ts_release {
				mux {
					pins = "gpio13", "gpio12";
					function = "gpio";
				};

				config {
					pins = "gpio13", "gpio12";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* PULL DOWN */
				};
			};
		};

		ext_buck_vsel {
			ext_buck_vsel_default: vsel0 {
				mux {
					pins = "gpio111";
					function = "gpio";
				};

				config {
					pins = "gpio111";
					drive-strength = <2>; /* 2 mA */
				};
			};
		};

		ext-cdc-tlmm-lines {
			ext_cdc_tlmm_lines_act: tlmm_lines_on {
				mux {
					pins = "gpio116", "gpio112",
							"gpio117", "gpio118",
							"gpio119";
					function = "gpio";
				};

				config {
					pins = "gpio116", "gpio112",
							"gpio117", "gpio118",
							"gpio119";
					drive-strength = <8>; /* 8 mA */
				};
			};

			ext_cdc_tlmm_lines_sus: tlmm_lines_off {
				mux {
					pins = "gpio116", "gpio112",
							"gpio117", "gpio118",
							"gpio119";
					function = "gpio";
				};

				config {
					pins = "gpio116", "gpio112",
							"gpio117", "gpio118",
							"gpio119";
					drive-strength = <2>; /* 2 mA */
					bias-disable; /* No PULL */
