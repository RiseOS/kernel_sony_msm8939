				};

				config {
					pins = "gpio63", "gpio64", "gpio65",
							"gpio66", "gpio67", "gpio68";
					drive-strength = <8>; /* 8 mA */
					bias-pull-none;
				};
			};
			cdc_pdm_lines_sus: pdm_lines_off {
				mux {
					pins = "gpio63", "gpio64", "gpio65",
							"gpio66", "gpio67", "gpio68";
					function = "cdc_pdm0";
