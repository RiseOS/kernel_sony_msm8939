	qcom,pm@8600664 {
		compatible = "qcom,pm";
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		reg = <0x8600664 0x80>;
		clock-names =
			"cpu0_clk", "cpu1_clk", "cpu2_clk",
			"cpu3_clk", "cpu4_clk", "cpu5_clk",
			"cpu6_clk", "cpu7_clk", "l2_clk";
		clocks = <&clock_cpu clk_a53_bc_clk>,
			 <&clock_cpu clk_a53_bc_clk>,
			 <&clock_cpu clk_a53_bc_clk>,
			 <&clock_cpu clk_a53_bc_clk>,
			 <&clock_cpu clk_a53_lc_clk>,
			 <&clock_cpu clk_a53_lc_clk>,
			 <&clock_cpu clk_a53_lc_clk>,
			 <&clock_cpu clk_a53_lc_clk>,
			 <&clock_cpu clk_cci_clk>;

		qcom,tz-flushes-cache;
	};

