					qcom,pm-cpu-level@2 {
						reg = <2>;
						qcom,spm-cpu-mode = "pc";
						qcom,latency-us = <260>;
						qcom,ss-power = <530>;
						qcom,energy-overhead = <400000>;
						qcom,time-overhead = <520>;
